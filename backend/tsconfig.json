{
    "compilerOptions": {
      "target": "ESNext",
      "useDefineForClassFields": true,
      "module": "CommonJS",
      "lib": ["ESNext", "DOM"],
      "moduleResolution": "Node",
      "strict": true,
      "resolveJsonModule": true,
      "isolatedModules": false,
      "esModuleInterop": true,
      "noEmit": true,
      "noImplicitReturns": true,
      "skipLibCheck": true
    }
  }
  